{
    "version": 2,
    "builds": [
        {
            "src": "views/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/vercel.ts",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/vercel.ts"
        }
    ]
}